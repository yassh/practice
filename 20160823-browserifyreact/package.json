{
  "name": "20160823-browserifyreact",
  "version": "1.0.0",
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "fixpack": "^2.3.1",
    "http-server": "^0.9.0",
    "jade-cli": "^0.1.1",
    "npm-run-all": "^3.0.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "uglifyify": "^3.0.2",
    "watchify": "^3.7.0"
  },
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "fixpack": "fixpack",
    "jade:watch": "jade ./src --out ./dist --pretty --watch",
    "jsx:watch": "watchify -t [ babelify ] -g [ uglifyify ] ./src/index.jsx -o ./dist/index.js",
    "serve": "http-server ./dist -p 8888 -o",
    "start": "npm-run-all -p *:watch serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
