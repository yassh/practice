    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4"
  },
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dest",
    "fix": "npm-run-all fixpack *:fix",
